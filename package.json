{
  "name": "hr-sm",
  "version": "1.0.0",
  "type": "module",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/index.js",
    "server": "nodemon server/index.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\" ",
    "test:routes": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPatterns=__tests__/routes",
    "test:auth": "node server/testing/test-authenticated-routes.js",
    "test:comprehensive": "node server/testing/test-all-routes.js",
    "test:all": "node server/testing/run-all-api-tests.js",
    "seed": "node server/seed.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "test:models": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPatterns=__tests__/models",
    "test:controllers": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPatterns=__tests__/controllers",
    "test:middleware": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPatterns=__tests__/middleware",
    "test:report": "node scripts/auto-test-report.js",
    "test:report:generate": "node scripts/generate-test-report.js",
    "test:comprehensive:report": "node run-comprehensive-tests.js",
    "test:comprehensive:full": "node run-all-tests.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "A comprehensive Human Resources Management System built with the MERN stack (MongoDB, Express.js, React, Node.js) designed to streamline HR operations, automate administrative tasks, and enhance employee management processes.",
  "devDependencies": {
    "concurrently": "^9.2.1",
    "jest": "^30.2.0",
    "jest-junit": "^16.0.0",
    "mongodb-memory-server": "^10.3.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cookieparser": "^0.1.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.19.2"
  }
}