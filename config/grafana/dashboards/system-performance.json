{
  "dashboard": {
    "id": null,
    "title": "HRMS System Performance Dashboard",
    "tags": ["system", "performance", "hrms", "monitoring"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Backend Services Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"hrms-main-backend\"}",
            "legendFormat": "Main Backend",
            "refId": "A"
          },
          {
            "expr": "up{job=\"hrms-license-server\"}",
            "legendFormat": "License Server",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            },
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "DOWN"
                  },
                  "1": {
                    "text": "UP"
                  }
                },
                "type": "value"
              }
            ]
          }
        },
        "gridPos": {
          "h": 4,
          "w": 8,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "CPU Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "100 - (avg(irate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
            "legendFormat": "System CPU Usage %",
            "refId": "A"
          },
          {
            "expr": "rate(process_cpu_seconds_total{job=\"hrms-main-backend\"}[5m]) * 100",
            "legendFormat": "Main Backend CPU %",
            "refId": "B"
          },
          {
            "expr": "rate(process_cpu_seconds_total{job=\"hrms-license-server\"}[5m]) * 100",
            "legendFormat": "License Server CPU %",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "CPU Usage %",
            "min": 0,
            "max": 100,
            "unit": "percent"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 8,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "(1 - (node_memory_MemAvailable_bytes / node_memory_MemTotal_bytes)) * 100",
            "legendFormat": "System Memory Usage %",
            "refId": "A"
          },
          {
            "expr": "process_resident_memory_bytes{job=\"hrms-main-backend\"} / 1024 / 1024",
            "legendFormat": "Main Backend Memory (MB)",
            "refId": "B"
          },
          {
            "expr": "process_resident_memory_bytes{job=\"hrms-license-server\"} / 1024 / 1024",
            "legendFormat": "License Server Memory (MB)",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Memory",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 8,
          "x": 16,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "Request Rate (req/sec)",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"hrms-main-backend\"}[5m])",
            "legendFormat": "Main Backend - {{method}} {{status}}",
            "refId": "A"
          },
          {
            "expr": "rate(http_requests_total{job=\"hrms-license-server\"}[5m])",
            "legendFormat": "License Server - {{method}} {{status}}",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "Response Time (95th percentile)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"hrms-main-backend\"}[5m]))",
            "legendFormat": "Main Backend",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"hrms-license-server\"}[5m]))",
            "legendFormat": "License Server",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Seconds",
            "min": 0,
            "unit": "s"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 6,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{job=\"hrms-main-backend\",status=~\"5..\"}[5m])",
            "legendFormat": "Main Backend 5xx",
            "refId": "A"
          },
          {
            "expr": "rate(http_requests_total{job=\"hrms-license-server\",status=~\"5..\"}[5m])",
            "legendFormat": "License Server 5xx",
            "refId": "B"
          },
          {
            "expr": "rate(http_requests_total{job=\"hrms-main-backend\",status=~\"4..\"}[5m])",
            "legendFormat": "Main Backend 4xx",
            "refId": "C"
          },
          {
            "expr": "rate(http_requests_total{job=\"hrms-license-server\",status=~\"4..\"}[5m])",
            "legendFormat": "License Server 4xx",
            "refId": "D"
          }
        ],
        "yAxes": [
          {
            "label": "Errors/sec",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 7,
        "title": "Database Connections",
        "type": "graph",
        "targets": [
          {
            "expr": "mongodb_connections_current{job=\"mongodb-exporter\"}",
            "legendFormat": "Current Connections",
            "refId": "A"
          },
          {
            "expr": "mongodb_connections_available{job=\"mongodb-exporter\"}",
            "legendFormat": "Available Connections",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Connections",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 8,
        "title": "Redis Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(redis_commands_processed_total[5m])",
            "legendFormat": "Commands/sec",
            "refId": "A"
          },
          {
            "expr": "redis_memory_used_bytes / 1024 / 1024",
            "legendFormat": "Memory Usage (MB)",
            "refId": "B"
          },
          {
            "expr": "redis_connected_clients",
            "legendFormat": "Connected Clients",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Commands/sec & Clients",
            "min": 0
          },
          {
            "label": "Memory (MB)",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 9,
        "title": "Disk I/O",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(node_disk_read_bytes_total[5m]) / 1024 / 1024",
            "legendFormat": "Read MB/s - {{device}}",
            "refId": "A"
          },
          {
            "expr": "rate(node_disk_written_bytes_total[5m]) / 1024 / 1024",
            "legendFormat": "Write MB/s - {{device}}",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "MB/s",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 24
        }
      },
      {
        "id": 10,
        "title": "Network I/O",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(node_network_receive_bytes_total[5m]) / 1024 / 1024",
            "legendFormat": "Receive MB/s - {{device}}",
            "refId": "A"
          },
          {
            "expr": "rate(node_network_transmit_bytes_total[5m]) / 1024 / 1024",
            "legendFormat": "Transmit MB/s - {{device}}",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "MB/s",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 32
        }
      },
      {
        "id": 11,
        "title": "WebSocket Connections",
        "type": "stat",
        "targets": [
          {
            "expr": "websocket_connections_active",
            "legendFormat": "Active Connections",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 500
                },
                {
                  "color": "red",
                  "value": 1000
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 12,
          "y": 32
        }
      },
      {
        "id": 12,
        "title": "Process Uptime",
        "type": "stat",
        "targets": [
          {
            "expr": "process_uptime_seconds{job=\"hrms-main-backend\"}",
            "legendFormat": "Main Backend",
            "refId": "A"
          },
          {
            "expr": "process_uptime_seconds{job=\"hrms-license-server\"}",
            "legendFormat": "License Server",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 3600
                },
                {
                  "color": "green",
                  "value": 86400
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 18,
          "y": 32
        }
      }
    ],
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
    "schemaVersion": 30,
    "version": 1,
    "links": []
  }
}
